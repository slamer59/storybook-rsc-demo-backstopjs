{
  "private": true,
  "imports": {
    "#app/actions": {
      "storybook": "./app/actions.mock.ts",
      "default": "./app/actions.ts"
    },
    "#libs/get-user-from-session": {
      "storybook": "./libs/get-user-from-session.mock.ts",
      "default": "./libs/get-user-from-session.ts"
    },
    "#prisma/prisma": {
      "storybook": "./prisma/prisma.mock.ts",
      "default": "./prisma/prisma.ts"
    },
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx"
    ]
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "prisma:setup": "dotenv -e .env.local prisma migrate dev",
    "prisma:seed": "dotenv -e .env.local prisma db seed",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm run build-storybook --quiet && http-server storybook-static --port 6006 --silent\" \"wait-on http://127.0.0.1:6006/ && pnpm run test-storybook\"",
    "generate-dmmf": "tsx prisma/generate-dmmf.ts"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts && tsx ./prisma/generate-dmmf.ts"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "cookie-signature-edge": "^1.0.7",
    "date-fns": "^3.6.0",
    "marked": "^1.2.9",
    "ms": "^2.1.3",
    "next": "14.1.4",
    "oauth": "^0.10.0",
    "react": "^18",
    "react-dom": "^18",
    "sanitize-html": "^2.13.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.25",
    "@prisma/client": "^5.11.0",
    "@storybook/addon-actions": "0.0.0-pr-26688-sha-5f6b1107",
    "@storybook/addon-essentials": "0.0.0-pr-26688-sha-5f6b1107",
    "@storybook/addon-interactions": "0.0.0-pr-26688-sha-5f6b1107",
    "@storybook/blocks": "0.0.0-pr-26688-sha-5f6b1107",
    "@storybook/nextjs": "0.0.0-pr-26688-sha-5f6b1107",
    "@storybook/react": "0.0.0-pr-26688-sha-5f6b1107",
    "@storybook/test": "0.0.0-pr-26688-sha-5f6b1107",
    "@storybook/test-runner": "^0.17.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-plugin-storybook": "^0.8.0",
    "http-server": "^14.1.1",
    "playwright": "^1.43.0",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "prisma-mock": "^0.10.0",
    "storybook": "0.0.0-pr-26688-sha-5f6b1107",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "wait-on": "^7.2.0"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
